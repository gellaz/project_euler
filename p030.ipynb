   "source": [
    "result = []\n",
    "\n",
    "for num in range(1000000):\n",
    "    _sum = 0\n",
    "    num_str = str(num)\n",
    "    \n",
    "    for digit_char in num_str:\n",
    "        digit = int(digit_char)\n",
    "        _sum += digit**5\n",
    "        \n",
    "    if _sum == num:\n",
    "        result.append(num)\n",
    "\n",
    "sum(result) - 1"
   ]
  }
